{
    "name": "lemmy",
    "id": "lemmy",
    "packaging_format": 1,
    "description": {
        "en": "Link aggregator/Reddit clone for the fediverse",
        "fr": "AgrÃ©gateur de liens/clone Reddit pour le fedivers"
    },
    "version": "0.15.2~ynh3",
    "url": "https://join.lemmy.ml/",
    "upstream": {
        "license": "GPL-3.0",
        "website": "https://join-lemmy.org/",
        "demo": "https://lemmy.ml/",
        "admindoc": "https://join-lemmy.org/docs/en/",
        "code": "https://github.com/LemmyNet/lemmy"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "Anmol Sharma",
        "email": "",
        "url": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "sitename",
                "type": "string",
                "ask": {
                    "en": "Enter site name",
                    "fr": "Entrez le nom du site"
                },
                "example": "lemmy website"
            }
        ]
    }
}
